{
  "name": "geo-mcp-server",
  "version": "1.2.0",
  "description": "MCP server providing full access to the Geo protocol SDK for knowledge graph operations",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "geo-mcp-server": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rickycambrian/geo_mcp_server"
  },
  "keywords": [
    "mcp",
    "geo",
    "knowledge-graph",
    "grc-20",
    "geo-protocol",
    "model-context-protocol"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@geoprotocol/geo-sdk": "^0.9.0",
    "@geoprotocol/grc-20": "^0.4.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "viem": "^2.23.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
